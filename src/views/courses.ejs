<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <%- include('partials/head.ejs')%>
    
    <title>Virutal Courses</title>
</head>
<body id="body">
    
    <%- include('partials/navegation.ejs')%>

    <main>

        <section class="courses">

            <h1>Courses</h1>

            <div class="border">
                - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - 
                - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - 
                - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - 
            </div>

            <div class="courseArea">

                <% videosUrl.forEach(function(url) { %>
                    <div class="course">
                        <iframe width="560" height="315" 
                            src="https://www.youtube.com/embed/rbuYtrNUxg4" allowfullscreen>
                        </iframe>
                        <div class="courseTitle"><%= url %></div>
                        <div class="courseDescription">Lorem ipsum dolor sit amet.</div>
                        <button class="courseButton">Enter</button>
                    </div>
                <% }); %>
                
            </div>

        </section>

        <div class="border">
            - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - 
            - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - 
            - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - 
        </div>

    </main>

    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
    <script src="./effects.js"></script>
    <!-- <script src="./api.js"></script> -->
    
</body>
</html>